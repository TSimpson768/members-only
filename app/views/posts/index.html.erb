<%= render 'flash' %>
<h1>Posts</h1>



  <% @posts.each do |post| %>
    <div>
      <h2><%= post.title %></h2>
      <h3>By: <%= render partial: 'author', :locals => {:username => post.user.username} %> </h3>
      <%= link_to 'Show', post %>
    </div>
  <% end %>


<br>

<%= link_to 'New Post', new_post_path %>
